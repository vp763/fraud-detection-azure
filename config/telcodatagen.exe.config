
<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- 
      Replace YOUR_EVENT_HUB_NAME with your Event Hub name 
      Example: telcocalls
    -->
    <add key="EventHubName" value="YOUR_EVENT_HUB_NAME" />

    <!-- 
      Replace YOUR_CONNECTION_STRING with your Event Hub connection string
      Get it from: Azure Portal → Event Hub Namespace → Shared Access Policies → RootManageSharedAccessKey → Connection string primary key
      Format: Endpoint=sb://YOURNAMESPACE.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=YOURKEY
    -->
    <add key="Microsoft.ServiceBus.ConnectionString" value="YOUR_CONNECTION_STRING_HERE" />
  </appSettings>
</configuration>
